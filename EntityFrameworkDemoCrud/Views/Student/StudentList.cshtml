@model List<EntityFrameworkDemoCrud.Context.Tbl_Student>

@{
    ViewBag.Title = "StudentList";
}

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>fname</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Desccription</th>
        <th>Action</th>
    </tr>
    
    

    
   @foreach (var item in Model)
   {
      
    <tr>
        <td>@item.Name</td>
        <td>@item.FName</td>
        <td>@item.Email</td>
        <td>@item.Mobile</td>
        <td>@item.Description</td>
        <td>
            <a href="#"><i class="glyphicon glyphicon-pencil"></i></a>
            <a href="#"><i class="glyphicon glyphicon-trash" ></i></a>
        </td>
    </tr>
    }
</table>

